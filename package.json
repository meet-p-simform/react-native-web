    "webpack-dev-server": "^4.15.1"
  },
  "resolutions": {
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "reactNativePermissionsIOS": [
    "Camera",
    "PhotoLibrary"
  ]
}